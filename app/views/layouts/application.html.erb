<!DOCTYPE html>
<html>
<head>
  <title>The Digital Shop</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="wrapper">
		<div id="page">
		    <div id="header">
				<div id="navcontainer">
					<ul id="navlist">
					  <li><%= link_to "Shops", shops_path %></li>
					  <li><%= link_to "Clients", clients_path %></li>
					  <li><%= link_to "Work Orders", work_orders_path %></li>
					  <li><%= link_to "Employees", employees_path %></li>
					  <li><%= link_to "Makes", makes_path %></li>
					  <li><%= link_to "Models", models_path %></li>
					</ul>
				</div>
			</div>
			<div id="content">
			<%= yield %>
			</div>
			<div id="footer">
			</div>
		</div>
	</div>
</body>
</html>
